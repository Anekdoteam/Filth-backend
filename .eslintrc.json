{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
            "semi": ["warn", "always"],
            "no-unused-vars": ["off", {"args": "after-used"}],
            "no-redeclare": ["off"]
    }
}
